{
    "name": "Parati Timesheet Filler"
    , "version": "0.1"
    , "manifest_version": 2
    , "description": "Timesheet hapens!"
    , "browser_action": {
        "default_icon": "icons/icon.png"
        , "default_title": "Insert your timesheet for batch inputs"
    }
    , "background": {
        "scripts": ["timesheet_background.js"]
        , "persistent": false
    }
    , "icons": {
        "16": "icons/icon16.png"
        , "32": "icons/icon32.png"
        , "48": "icons/icon48.png"
        , "72": "icons/icon72.png"
        , "128": "icons/icon128.png"
    }
    , "permissions": ["downloads", "tabs", "activeTab"]
    , "externally_connectable": {
        "matches": [
      "http://parati.trescon.com.br/Sistema3con/parati.aspx"
    ]
    }
    , "content_scripts": [{
        "matches": ["http://parati.trescon.com.br/Sistema3con/parati.aspx"]
        , "js": ["timesheet_content_script.js"]
  }]
    , "web_accessible_resources": ["timesheet_content_script.js"]
}